# Bitcoin Network Configuration
NEXT_PUBLIC_DEFAULT_NETWORK=regtest

# Google Drive Backup (OAuth2)
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# GitHub OAuth2 Authentication
NEXT_PUBLIC_GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# External Database for Email Authorization
EXTERNAL_DB_URL=https://your-database-api.com
EXTERNAL_DB_API_KEY=your-database-api-key

# Breez SDK API Key (server-only, fetched at runtime to avoid exposure in client bundle)
BREEZ_API_KEY=
